port 465
from leoaparisi@gmail.com
user leoaparisi@gmail.com
passwordeval "pass leoaparisi@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/leoap/.cache/msmtp/msmtp.log
tls_starttls off

