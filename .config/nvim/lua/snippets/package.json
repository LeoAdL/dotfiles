{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintex",
          "latex",
          "tex"
        ],
        "path": "./snip/latex.json"
      },
      {
        "language": [
          "markdown"
        ],
        "path": "./snip/md.json"
      }
    ]
  }
}
